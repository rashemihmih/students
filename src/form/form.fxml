                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
      <TabPane fx:id="groupTabPane" layoutX="430.0" layoutY="10.0" prefHeight="420.0" prefWidth="415.0" tabClosingPolicy="UNAVAILABLE" visible="false">
        <tabs>
          <Tab text="Информация">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="tfGroupInfoName" editable="false" layoutX="158.0" layoutY="10.0" />
                        <Label layoutX="14.0" layoutY="14.0" text="Название" />
                        <TextField fx:id="tfGroupInfoStudents" editable="false" layoutX="158.0" layoutY="42.0" />
