                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Удалить">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="182.0" layoutY="211.0" mnemonicParsing="false" onMouseClicked="#deleteStudent" text="Удалить" />
                     </children>
                  </AnchorPane>
               </content>
