                        <TextField fx:id="tfAddStudentFirstName" layoutX="158.0" layoutY="42.0" />
                        <Label layoutX="14.0" layoutY="46.0" text="Имя" />
                        <TextField fx:id="tfAddStudentRating" layoutX="158.0" layoutY="108.0" />
                        <Label layoutX="14.0" layoutY="112.0" text="Рейтинг" />
                        <TextField fx:id="tfAddStudentMiddleName" layoutX="158.0" layoutY="76.0" />
                        <Label layoutX="14.0" layoutY="80.0" text="Отчество" />
                        <Button layoutX="158.0" layoutY="143.0" mnemonicParsing="false" onMouseClicked="#addStudent" prefHeight="25.0" prefWidth="150.0" text="Добавить" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Переименовать">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="tfGroupNewName" layoutX="84.0" layoutY="14.0" />
                        <Label layoutX="14.0" layoutY="18.0" text="Название" />
                        <Button layoutX="84.0" layoutY="51.0" mnemonicParsing="false" onMouseClicked="#renameGroup" prefHeight="25.0" prefWidth="150.0" text="Переименовать" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
            <Tab text="Удалить группу">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="182.0" layoutY="211.0" mnemonicParsing="false" onMouseClicked="#deleteGroup" text="Удалить" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
      <TabPane fx:id="studentTabPane" layoutX="430.0" layoutY="10.0" prefHeight="420.0" prefWidth="415.0" tabClosingPolicy="UNAVAILABLE" visible="false">
