<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="495.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="form.Controller">
   <children>
      <TreeView fx:id="treeView" layoutX="10.0" layoutY="10.0" prefHeight="475.0" prefWidth="400.0" />
      <Button layoutX="430.0" layoutY="460.0" mnemonicParsing="false" onMouseClicked="#loadData" text="Загрузить данные" />
